<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel width="520px">
		<g:VerticalPanel width="100%" spacing="1"
			horizontalAlignment="ALIGN_LEFT">
			<g:HorizontalPanel width="100%" spacing="3">
				<g:Label text="Proveedor" width="240px" height="" />
				<g:Label text="Cliente" width="240px" height="" />
			</g:HorizontalPanel>
			<g:HorizontalPanel width="100%" height="20px"
				spacing="3">
				<g:ListBox ui:field="cbProveedor" width="240px" height="20px" />
				<g:ListBox ui:field="cbCliente" width="240px" height="20px" />
			</g:HorizontalPanel>
			<g:HorizontalPanel width="100%" spacing="3">
				<g:Label text="Toneladas" width="240px" />
				<g:Label text="Precio por tonelada de madera" width="240px" />
			</g:HorizontalPanel>
			<g:HorizontalPanel width="100%" height="20px"
				spacing="3">
				<g:TextBox ui:field="tbKilos" width="240px" height="20px" />
				<g:TextBox ui:field="tbPrecioXKilo" width="240px" height="20px" />
			</g:HorizontalPanel>
			<g:HorizontalPanel width="100%" spacing="3">
				<g:Label text="Fletero" width="240px" />
				<g:Label text="Precio por tonelada del flete" width="240px" />
			</g:HorizontalPanel>
			<g:HorizontalPanel width="100%" height="20px"
				spacing="3">
				<g:ListBox ui:field="cbFletero" width="246px" height="20px" />
				<g:TextBox height="20px" width="240px" ui:field="tbPrecioToneladaFlete" />
			</g:HorizontalPanel>
			<g:HorizontalPanel width="100%" spacing="3">
				<g:Label text="Descargador" width="240px" />
				<g:Label text="Monto" width="240px" />
			</g:HorizontalPanel>
			<g:HorizontalPanel width="100%" height="20px">
				<g:ListBox ui:field="cbDescargador" width="246px" height="20px" />
				<g:TextBox ui:field="tbMontoDescargador" width="240px"
					height="20px" />
			</g:HorizontalPanel>
			<g:HorizontalPanel width="100%" spacing="3">
				<g:Label text="Empresa" width="240px" />
				<g:ListBox ui:field="cbEmpresaPropia" width="246px" height="20px" />
			</g:HorizontalPanel>
			<g:HorizontalPanel width="100%" spacing="3">
				<g:Label text="Tipo de carga" width="240px" />
				<g:Label text="Factura" width="240px" />
			</g:HorizontalPanel>
			<g:HorizontalPanel width="100%" height="20px"
				spacing="3">
				<g:TextBox height="20px" width="240px" ui:field="tbTipoCarga" />
				<g:TextBox height="20px" width="240px" ui:field="tbFactura" />
			</g:HorizontalPanel>
			<g:Label ui:field="errorLabel" width="100%" height="20px" />
			<g:HorizontalPanel width="100%"
				horizontalAlignment="ALIGN_CENTER">
				<g:Button width="250px" height="30px" ui:field="btnAceptar">Aceptar</g:Button>
			</g:HorizontalPanel>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 