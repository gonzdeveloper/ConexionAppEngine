<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>		
	/* stock list flex table */
	.watchList {
		border: 1px solid silver;
		padding: 2px;
		margin-bottom: 6px;
	}
	</ui:style>
	<g:HTMLPanel width="800px">
		<g:VerticalPanel width="100%" height="100%">
			<g:HorizontalPanel spacing="6">
				<g:Button ui:field="btnAgregar" width="69">Agregar</g:Button>
				<g:Button width="69" ui:field="btnModificar">Modificar</g:Button>
				<g:Button width="69" ui:field="btnEliminar">Eliminar</g:Button>
			</g:HorizontalPanel>
			<g:DialogBox ui:field="dialogBox"></g:DialogBox>
			<g:ScrollPanel>
				<g:FlexTable ui:field="tablaEmpresas" addStyleNames="{style.watchList}"
					width="100%" />
			</g:ScrollPanel>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 